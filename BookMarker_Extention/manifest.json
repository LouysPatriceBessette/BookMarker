{
  "manifest_version": 2,
  "name": "Bookmarker.club - Easy add link",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {"48":"sept.png"}
  },
  "permissions": [
    "tabs"
  ]
}